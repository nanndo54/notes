{
  "name": "notes",
  "version": "1.0.0",
  "private": "true",
  "author": "pabloc54 <pablofernando54@outlook.com>",
  "main": "index.js",
  "repository": "git@github.com:pabloc54/notes.git",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "start": "node docs/server/index.js",
    "dev": "nodemon docs/server/index.js",
    "dev:client": "cd src/client && vite",
    "build": "cd src/server && tsc --watch",
    "build:client": "cd src/client && vite build --watch",
    "serve": "vite preview",
    "lint:fix": "eslint ./src --ext .jsx,.js,.ts,.tsx --quiet --fix",
    "lint:format": "prettier --loglevel warn --write \"./**/*.{js,jsx,ts,tsx,css,md,json}\"",
    "lint": "npm run lint:format && npm run lint:fix"
  },
  "dependencies": {
    "@reduxjs/toolkit": "1.6.0",
    "dotenv": "10.0.0",
    "express": "4.17.1",
    "morgan": "1.10.0",
    "react": "17.0.0",
    "react-dom": "17.0.0",
    "react-hook-form": "7.9.0",
    "react-redux": "7.2.4",
    "redux-devtools-extension": "2.13.9",
    "vite-tsconfig-paths": "3.3.13",
    "wouter": "2.7.4"
  },
  "devDependencies": {
    "@types/babel__core": "^7.1.16",
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "@types/node": "16.11.8",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/react-redux": "7.1.16",
    "@types/redux-form": "8.3.1",
    "@typescript-eslint/eslint-plugin": "4.17.0",
    "@typescript-eslint/parser": "4.17.0",
    "@vitejs/plugin-react": "1.0.7",
    "eslint": "7.29.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "nodemon": "2.0.12",
    "pre-commit": "1.2.2",
    "prettier": "2.3.2",
    "typescript": "4.3.5",
    "vite": "2.6.14"
  },
  "pre-commit": "lint"
}
